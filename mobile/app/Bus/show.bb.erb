<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @bus.seats %></h1>
    <%= link_to "Edit", :action => :edit, :id => @bus.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Seats</td>
        <td class="itemValue"><%= @bus.seats%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CompanyId</td>
        <td class="itemValue"><%= @bus.company_id%></td>
      </tr>
      
    </table>
  </div>

</div>
